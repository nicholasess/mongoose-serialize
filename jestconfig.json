
  {
    "modulePaths": [
      "<rootDir>"
    ],
    "testEnvironment": "node",
    "moduleFileExtensions": [
      "ts",
      "js",
      "json"
    ],
    "transform": {
      ".(js|jsx|ts|tsx)": "ts-jest"
    },
    "testMatch": [
      "**/__tests__/*.spec.+(ts|js)",
      "**/__tests__/*/*.spec.+(ts|js)"
    ],
    "coverageReporters": [
      "text",
      "cobertura",
      "lcov"
    ],
    "reporters": [
      "default"
    ]
  }